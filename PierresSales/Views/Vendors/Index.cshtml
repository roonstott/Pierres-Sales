@{
  Layout = "_Layout";
}
@using PierresSales.Models

@if (Model.Count ==0) 
{
  <h3>No Vendors Listed Yet</h3>
  <a href="/vendors/new">Click here to add a vendor</a>
}

<ul>
@foreach(KeyValuePair<int, Vendor> ven in Model)
{
  <li><a href="vendors/@ven.Value.ClientNumber">@ven.Value.Name</a></li>
}

</ul>